{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://wegenenverkeer.be/api/dataloader/model/feedReaderStatus.json",
  "type": "object",
  "title": "Feed Reader status",
  "description": "Schema voor een feed reader status.",
  "properties": {
    "feedNaam": {
      "type": "string"
    },
    "lastUpdate": {
      "type": "string"
    },
    "status": {
      "enum": [
        "created",
        "running",
        "waiting",
        "idle",
        "failing on source",
        "failing on target",
        "failing on database",
        "consumer busy with other feeds"
      ]
    },
    "lastSuccess": {
      "object": "syncStatus",
      "$ref": "syncStatus.json"
    }
  },
  "required": [
    "feedNaam",
    "lastUpdate",
    "status"
  ]
}
